"""Format our source files."""

import sys

import terminal
import libdot  # pylint: disable=wrong-import-order


def main(argv: list[str]) -> int:
    """The main func!"""
    return libdot.format_tool.main(argv, basedir=terminal.DIR)


if __name__ == "__main__":
    sys.exit(main(sys.argv[1:]))
